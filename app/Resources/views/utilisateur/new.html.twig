{% extends 'base.html.twig' %}

{% block body %}
    <h1>Création utilisateur</h1>
    <br/>
    {{ form_start(form) }}
    <div class="form-group">
        {{ form_widget(form) }}
        <input type="submit" value="Enregistrer" class="btn btn-success " />
